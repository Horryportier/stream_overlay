; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="stream_overlay"
run/main_scene="uid://1axspijg5cxc"
config/features=PackedStringArray("4.4", "Forward Plus")
config/icon="res://icon.svg"

[autoload]

Net="*res://autoload/net.gd"

[display]

window/size/viewport_width=1280
window/size/viewport_height=720
window/size/resizable=false
window/size/always_on_top=true
window/stretch/mode="viewport"

[editor_plugins]

enabled=PackedStringArray("res://addons/ldtk-importer/plugin.cfg")

[file_customization]

folder_colors={
"res://assets/": "purple",
"res://exports/": "orange",
"res://resources/": "red",
"res://scenes/": "green",
"res://scripts/": "yellow"
}

[importer_defaults]

ldtk.import={
"Debug": "",
"Entity": "",
"Layer": "",
"Level": "",
"Post Import": "",
"Tileset": "",
"World": "",
"atlas_texture_type": 0,
"entities_post_import": "",
"force_tileset_reimport": false,
"group_world_layers": false,
"integer_grid_tilesets": false,
"layers_always_visible": false,
"level_post_import": "uid://xvvavs8ljw0q",
"pack_levels": true,
"resolve_entityrefs": true,
"tileset_custom_data": false,
"tileset_post_import": "",
"use_entity_placeholders": false,
"verbose_output": false,
"world_post_import": ""
}

[rendering]

textures/canvas_textures/default_texture_filter=0
2d/snap/snap_2d_transforms_to_pixel=true
